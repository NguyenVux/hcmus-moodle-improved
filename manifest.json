{
  "name": "HCMUS Moodle Enhanced",
  "description": "",
  "version": "1.0.2",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_title": "Moodle settings",
    "default_popup": "src/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://courses.ctda.hcmus.edu.vn/login/index.php*",
        "https://courses.hcmus.edu.vn/login/index.php*"
      ],
      "js": [
        "src/jquery.js",
        "src/Main.js"
      ]
    }
  ]
}
